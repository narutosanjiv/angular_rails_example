<div my-directive my-url="http://google.com" my-link-text="Click me to go to Google" some-text='aaaaaaaaa'></div>
<div class='row'>
  <button class="btn btn-default btn-info" ng-click="add_to_list()">Add</button>
  <a class="btn btn-default btn-success" href="#/form"> Form </a> 
</div>
<ng-view></ng-view>
<!-- directive: my-directive -->
<div class="page-header">
  <h1>{{todo.name}}'s To Do List</h1>
  <span class="label label-default" ng-hide="incompleteCount() == 0">
    {{incompleteCount()}}
  </span>
</div>
<div class="panel">
  <div class="input-group">
    <input class="form-control" />
    <span class="input-group-btn">
      <button class="btn btn-default btn-info" ng-click="add_to_list()">Add</button>
    </span>
  </div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Description</th>
        <th>Done</th>
      </tr>
    </thead>
    <tbody>
      <tr ng-repeat="item in todo.items">
        <td> {{item.desc}}</td>
        <td><input type="checkbox" ng-model="item.done" /></td>
        <td> {{item.done}}</td>
        <td>  
          <a class="btn btn-danger" href="#">
            <i class="icon-trash icon-white"></i>
            Delete
          </a>
        </td>
      </tr>
    </tbody>
  </table>
  <div class="container-fluid" ng-include src="'template'"> </div>
</div>
